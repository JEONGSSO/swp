<?xml version="1.0" encoding="UTF-8"?>

<!-- DTD 선언 -->
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="UserMapper">

	<!-- 행 수정 -->
	<select id="login">
		select * from user where uid = {#uid} and upw = password(#{upw});
	</select>
	
</mapper>